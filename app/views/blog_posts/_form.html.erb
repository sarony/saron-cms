<%= form_for @blog_post do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :subtitle %>
  <%= f.text_field :subtitle %>

  <%= f.label :body %>
  <%= f.text_area :body %>

  <%= f.label :preview %>
  <%= f.text_area :preview %>

  <%= f.label :cover_art %>
  <%= f.text_field :cover_art %>

  <%= f.submit %>
<% end %>